{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "朋友圈",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/settings/index",
      "style": {
        "navigationBarTitleText": "个人中心"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#3a6ce5",
    "backgroundColor": "#ffffff"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#6399e1",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabBarIcon/home.png",
        "selectedIconPath": "static/tabBarIcon/home-active.png",
        "text": "朋友圈"
      },
      {
        "pagePath": "pages/settings/index",
        "iconPath": "static/tabBarIcon/message.png",
        "selectedIconPath": "static/tabBarIcon/message-active.png",
        "text": "个人中心"
      }
    ]
  },

  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  }
}
